<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiExamApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:MauiExamApp.Views"
    Title="MauiExamApp">

    <TabBar>
        <ShellContent
            Title="Dashboard"
            Icon="dashboard_icon.png"
            ContentTemplate="{DataTemplate views:DashboardView}" />

        <ShellContent
            Title="Historik"
            Icon="history_icon.png"
            ContentTemplate="{DataTemplate views:HistoryView}" />
    </TabBar>

</Shell>

